# OpenAI-compatible LLM endpoint (LLM Studio or cloud)
# Do not include /v1 in the base URL
LLM_BASE_URL=http://localhost:11434
LLM_API_KEY=

# Models
GENERAL_MODEL=gemma-2-9b-it
MED_MODEL=medgemma-2
ORCHESTRATION_MODEL= 
