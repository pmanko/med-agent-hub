# A2A simulation
ENABLE_A2A=true
CHAT_TIMEOUT_SECONDS=90

# OpenMRS FHIR (Agents mode)
OPENMRS_FHIR_BASE_URL=
OPENMRS_USERNAME=
OPENMRS_PASSWORD=

# Local FHIR parquet directory (Agents mode)
FHIR_PARQUET_DIR=
