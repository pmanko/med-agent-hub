web: UVICORN_ENV_FILE=.env poetry run uvicorn server.main:app --host 0.0.0.0 --port ${WEB_PORT:-8080} --reload --env-file .env >> logs/web.log 2>&1
router: UVICORN_ENV_FILE=.env poetry run uvicorn server.sdk_agents.router_server:app --host 0.0.0.0 --port ${A2A_ROUTER_PORT:-9100} --reload --env-file .env >> logs/router.log 2>&1
medical: UVICORN_ENV_FILE=.env poetry run uvicorn server.sdk_agents.medical_server:app --host 0.0.0.0 --port ${A2A_MEDICAL_PORT:-${A2A_MEDGEMMA_PORT:-9101}} --reload --env-file .env >> logs/medical.log 2>&1
clinical: UVICORN_ENV_FILE=.env poetry run uvicorn server.sdk_agents.clinical_server:app --host 0.0.0.0 --port ${A2A_CLINICAL_PORT:-9102} --reload --env-file .env >> logs/clinical.log 2>&1
admin: UVICORN_ENV_FILE=.env poetry run uvicorn server.sdk_agents.administrative_server:app --host 0.0.0.0 --port ${A2A_ADMIN_PORT:-9103} --reload --env-file .env >> logs/admin.log 2>&1
